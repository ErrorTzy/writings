@article{blazevPhosphoproteomicsThreeExercise2022,
  title = {Phosphoproteomics of Three Exercise Modalities Identifies Canonical Signaling and {{C18ORF25}} as an {{AMPK}} Substrate Regulating Skeletal Muscle Function},
  author = {Blazev, Ronnie and Carl, Christian S. and Ng, Yaan-Kit and Molendijk, Jeffrey and Voldstedlund, Christian T. and Zhao, Yuanyuan and Xiao, Di and Kueh, Andrew J. and Miotto, Paula M. and Haynes, Vanessa R. and Hardee, Justin P. and Chung, Jin D. and McNamara, James W. and Qian, Hongwei and Gregorevic, Paul and Oakhill, Jonathan S. and Herold, Marco J. and Jensen, Thomas E. and Lisowski, Leszek and Lynch, Gordon S. and Dodd, Garron T. and Watt, Matthew J. and Yang, Pengyi and Kiens, Bente and Richter, Erik A. and Parker, Benjamin L.},
