  pages = {108--120},
  issn = {1420-8008},
  doi = {10.1159/000093487},
  abstract = {The enhancing effect of music on autobiographical memory recall in mild Alzheimer's disease individuals (n = 10; Mini-Mental State Examination score {$>$}17/30) and healthy elderly matched individuals (n = 10; Mini-Mental State Examination score 25-30) was investigated. Using a repeated-measures design, each participant was seen on two occasions: once in music condition (Vivaldi's 'Spring' movement from 'The Four Seasons') and once in silence condition, with order counterbalanced. Considerable improvement was found for Alzheimer individuals' recall on the Autobiographical Memory Interview in the music condition, with an interaction for condition by group (p {$<$} 0.005). There were no differences in terms of overall arousal using galvanic skin response recordings or attentional errors during the Sustained Attention to Response Task. A significant reduction in state anxiety was found on the State Trait Anxiety Inventory in the music condition (p {$<$} 0.001), suggesting anxiety reduction as a potential mechanism underlying the enhancing effect of music on autobiographical memory recall.},
  langid = {english},
  pmid = {16717466},
  keywords = {Acoustic Stimulation,Aged,Alzheimer Disease,Analysis of Variance,Anxiety,Arousal,Attention,Cognition,Female,Galvanic Skin Response,Humans,Male,Memory,Mental Recall,Music,Music Therapy,Neuropsychological Tests,Psychomotor Performance,Reaction Time}
}

