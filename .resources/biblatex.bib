  author = {Cabeza, Roberto and Grady, Cheryl L. and Nyberg, Lars and McIntosh, Anthony R. and Tulving, Endel and Kapur, Shitij and Jennings, Janine M. and Houle, Sylvain and Craik, Fergus I. M.},
  date = {1997-01-01},
