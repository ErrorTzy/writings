@thesis{DongXuLinDangDaiErYuanLunFaZhanZhongDiQiaErSiXiangDeGuanChuanJiQiYanBian2017,
  type = {硕士},
  title = {当代二元论发展中笛卡尔思想的贯穿及其演变},
  author = {董胥琳},
  editora = {陈向澜},
  editoratype = {collaborator},
  date = {2017},
  institution = {苏州大学},
  url = {https://kns.cnki.net/kcms2/article/abstract?v=p7sfyaWOx3O59I_upLtWzLb47jKaijMz91T4JYAQTnNbNTAhLOwL_njBeryA-4oS25Cz1VdM8cN83_HxcEdxc1siTyOeSV_BN4Z9Fp97YBGOV9HbGGq3oA==&uniplatform=NZKPT&language=gb},
  abstract = {笛卡尔的实体二元论是二元论历史的重要一程或者说组成部分。当代二元论代表了西方心灵哲学发展的趋势之一。如今二元论发展得最为普遍的是就意识属性的存在论地位进行探讨的属性二元论,但是实体二元论的图式仍然存在着实体本体论上的考虑。所以两种二元论之间存在着区别,不仅如此,它们各自内部也存着差异和分化。但无论内容有何差异,其讨论主题仍然离不开三个部分,即心灵实体(在属性二元论的框架里为心灵状态)、生理身体和心物之间的关系。而这三个部分的内容早在笛卡尔的思想中就已经涉及到了,只不过随着时代的发展,哲学理论的深入让我们看到了笛卡尔所构建的跟这三部分相关的理论的不足之处。但是我们也应该看到当代二元论中仍有不少理论扬弃地使用了笛卡尔的观点,它们或是在新的论证基础上支持实体二元论的立场,或是根据其不足之处转换成新的二元论形式,或是借新兴科学之手重提心智二元结构。所以为了更加深入地了解当代二元论的发展路径,我们有必要根据笛卡尔的思想结构来探讨当代二元论内部的论证结构和理论基础。在凸出西方心灵哲学引领的当代二元论的同时,研究传统的笛卡尔式心身理论与西方当代二元论的异同点及其在未来(哲学科学实践中)的结合点,是当今心灵哲学研究在如何进一步引进、消化、吸收、扬弃传统心身理论方面的重要课题。},
  langid = {chinese},
  keywords = {二元论,心灵哲学,笛卡尔},
  annotation = {5 citations(CNKI)[17-12-2023]},
  file = {/home/scott/Zotero/storage/338MNJTE/当代二元论发展中笛卡尔思想的贯穿及其演变_董胥琳.pdf}
}

