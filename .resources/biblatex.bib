@article{liuMoodStatusResponse2023,
  title = {Mood {{Status Response}} to {{Physical Activity}} and {{Its Influence}} on {{Performance}}: {{Are Chronotype}} and {{Exercise Timing Affect}}?},
  shorttitle = {Mood {{Status Response}} to {{Physical Activity}} and {{Its Influence}} on {{Performance}}},
  author = {Liu, Hengxu and Liang, Jiaqi and Wang, Kun and Zhang, Tingran and Liu, Shiqi and Luo, Jiong},
