@article{backmanBrainActivationYoung1997,
  title = {Brain {{Activation}} in {{Young}} and {{Older Adults During Implicit}} and {{Explicit Retrieval}}},
  author = {B{\"a}ckman, Lars and Almkvist, Ove and Andersson, Jesper and Nordberg, Agneta and Winblad, Bengt and Reineck, Robert and L{\aa}ngstr{\"o}m, Bengt},
  year = {1997},
  month = may,
  journal = {Journal of Cognitive Neuroscience},
