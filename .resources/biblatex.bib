  author = {Sawczuk, Marek and Timshina, Yevgeniya I. and Astratenkova, Irina V. and Maciejewska-Karlowska, Agnieszka and Leo{\'n}ska-Duniec, Agata and Ficek, Krzysztof and Mustafina, Leysan J. and Ci{\k e}szczyk, Pawel and Klocek, Tomasz and Ahmetov, Ildus I.},
