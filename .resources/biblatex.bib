@article{gouldProliferationGranuleCell1998,
  title = {Proliferation of Granule Cell Precursors in the Dentate Gyrus of Adult Monkeys Is Diminished by Stress},
  author = {Gould, Elizabeth and Tanapat, Patima and McEwen, Bruce S. and Fl{\"u}gge, Gabriele and Fuchs, Eberhard},
  year = {1998},
